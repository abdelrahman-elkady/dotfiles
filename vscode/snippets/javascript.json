{
	"Print to console": {
		"prefix": "clog",
		"body": [
			"console.log($1);"
		],
		"description": "Log output to console"
	},

	"require": {
		"prefix": "req",
		"body": [
			"const $1 = require('$2');"
		],
		"description": "Require a module"
	},

	"require with same name": {
		"prefix": "reqn",
		"body": [
			"const $1 = require('$1');"
		],
		"description": "Require a module with its same name"
	},

	"JSON stringify to Console": {
		"prefix": "jsons",
		"body": [
			"console.log(JSON.stringify($1, null, 2))"
		],
		"description": "JSON stringify to Console"
	},

	"Module Exports": {
		"prefix": "mod",
		"body": [
			"module.exports = { $1 };"
		]
	},

	"Module Exports Default": {
		"prefix": "modf",
		"body": [
			"module.exports = $1;"
		]
	},

	"Describe Block": {
		"prefix": ["des", "desc"],
		"body": [
			"describe('$1', () => {",
			"\t$2",
			"});"
		]
	},

	"Mocha It test block": {
		"prefix": ["it"],
		"body": [
			"it('$1', async () => {",
			"\t$2",
			"});"
		]
	},

	"Use Strict Mode": {
		"prefix": ["strict"],
		"body": "'use strict';"
	},
}
