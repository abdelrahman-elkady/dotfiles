      "width": "alignToLast"
    },
    {
      "width": "alignToLast"
    },
    {
      "start": "current",
      "align": "right",
      "prefix": "<<<<",
      "suffix": ">>>>"
    },
    {
      "start": "current",
      "align": "right",
      "padding": "zero",
